<%= link_to "+ New Project",new_project_path,:class=>"btn btn-success pull-right"%>
<br/>
<br/>
<div>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Manage</th>
    </tr>
  <thead>
  <tbody>    
  <% @projects.each do |project| %>
    <tr>
      <td><%= project.name%></td>
      <td><%= link_to "Show",project_path(project)%> |
        <%= link_to "Edit",edit_project_path(project)%> |
        <%= link_to "Delete",project_path(project),:method=>:delete,:data=>{:confirm=>"Are You sure?"}%> |
      </td>
    </tr>
  <% end %>
  </tbody>
</table>  
</div>